echo $SHELL
who | wc -l  |

# Funciona interpretando tudo como string 
echo " Existem  `who | wc -l ` usuários conectados " 

# Funciona com o comando mais string 
echo  Existem  `who | wc -l ` usuários conectados 

# Redirecionamento de Arquivos 

ls | wc -l # mostra o numero de linhas 

